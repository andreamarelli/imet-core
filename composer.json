{
    "name": "andreamarelli/imet-core",
    "description": "Imet core package",
    "authors": [
        {
            "name": "Andrea Marelli",
            "email": "andrea.marelli.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "andreamarelli/modular-forms": "dev-namespace"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "repositories": {
        "andreamarelli/modular-forms": {
            "type": "vcs",
            "url": "https://github.com/andreamarelli/modular-forms"
        }
    },
    "autoload": {
        "psr-4": {
            "ImetCore\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ImetCore\\ServiceProvider"
            ],
            "aliases": {
                "ImetUser": "ImetCore\\Models\\User"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
